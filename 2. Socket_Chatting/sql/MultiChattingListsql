CREATE TABLE MULTICHATTINGLIST (
    MULTICHATTINGLIST_ID INT PRIMARY KEY,
    MULTICHATTINGROOM_ID INT,
    ID INT,
    CONSTRAINT FK_MULTICHATTINGLIST_ROOM FOREIGN KEY (MULTICHATTINGROOM_ID) REFERENCES MULTICHATTINGROOM(MULTICHATTINGROOM_ID),
    CONSTRAINT FK_MULTICHATTINGLIST_UID FOREIGN KEY (ID) REFERENCES MEMBER(ID)
);

CREATE SEQUENCE MULTICHATTINGLIST_SEQ START WITH 1 INCREMENT BY 1 NOCACHE;
